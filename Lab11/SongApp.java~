import javax.swing.*;
import java.awt.*;

public class SongApp {
  public static void main(String[]args){
    
    Song song1 = new Song("While my guitar gently weeps");
    System.out.println(song1.toString());
    song1.process();
    
    Song song2 = new Song("Let it be");
    System.out.println(song2.toString());
    song2.process();
    
  }

}