import javax.swing.*;
import java.awt.*;


public class BoxApp {
  public static void main(String[]args){
  
    Box box1 = new Box();
    box1.setHeight(4);
    box1.setLength(4);
    box1.setWidth(6);
    box1.ToString();
    
    Box box2 = new Box(3,4,5);
    box2.ToString();
    
    Box box3 = new Box(5,5,5);
    box3.ToString();
  }
  
  

}